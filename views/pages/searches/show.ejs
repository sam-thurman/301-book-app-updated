<!-- search results from API -->

<!DOCTYPE html>
<html lang="en">

<%- include('../../layout/head') %>

<body>
  <h1>Search Results</h1>
  <% bookArray.forEach(book => { %>
  <article>
    <img src="<%= book.image_url%>" alt="<%= book.title %>">
    <h2><%= book.title %></h2>
    <h4><%= book.author %></h4>
    <p class="book-summary"><%= book.summary %></p>
  </article>
  <% }) %>
</body>

</html>